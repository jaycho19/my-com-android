    android:layout_height="match_parent"
    android:orientation="vertical" >

    <include layout="@layout/fragment_trademark_top_bar" />

        <com.next.lottery.view.MyHorizontalScrollView
            android:id="@+id/fragment_trade_mark_horizontal_scroview"
